
pipeline {
	agent any
	stages {
	
		stage ('Checkout Code') {
			steps {
				echo 'Checkout code and perform mvn clean' 	
				script {
					bat `dir`					
				}
			}
		}
		
		stage ('Change Verification') {
			steps {
				echo 'Identify changes in branch [develop] w.r.t branch [master]...'				
			}
		}				
	}
}